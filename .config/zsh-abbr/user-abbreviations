abbr "abbr list"="cat ~/.config/zsh-abbr/user-abbreviations"
abbr "abbr ls"="cat ~/.config/zsh-abbr/user-abbreviations"
abbr "abbrlist"="cat ~/.config/zsh-abbr/user-abbreviations"
abbr "d"="docker"
abbr "dc"="docker compose"
abbr "dce"="docker compose exec"
abbr "docker compose d"="docker compose down"
abbr "ga"="git add"
abbr "gaa"="git add -A"
abbr "gb"="git branch"
abbr "gc"="git commit -v"
abbr "gd"="git diff"
abbr "git commit -v a"="git commit -v --amend"
abbr "git rebase a"="git rebase --abort"
abbr "git rebase c"="git rebase --continue"
abbr "gl"="git log"
abbr "gm"="git merge"
abbr "gplo"="git pull origin \$(git rev-parse --abbrev-ref HEAD)"
abbr "gpso"="git push origin \$(git rev-parse --abbrev-ref HEAD)"
abbr "gr"="git rebase"
abbr "src"="source ~/.zshrc"
